ion\/AdSDKCore\/AdService\/tagUser(request:)-2remz"},{"type":"text","text":" "},{"text":"(Combine) and ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/com.adsdk.core\/documentation\/AdSDKCore\/AdService\/tagUser(request:)-4qs0m"},{"text":" (Async\/Await) methods.","type":"text"},{"text":" ","type":"text"},{"text":"You can pass all tags through the initializer.","type":"text"}]},{"syntax":"swift","code":["\/\/ Combine","subscription = adService.tagUser(","    request: .init([","        .init(key: \"key\", subkey: \"subkey\", value: \"value\"),","        .init(key: \"key2\", subkey: \"subkey2\")","    ])",")",".sink { completion in","    switch completion {","    case .finished:","        print(\"Tagging finished\")","    case .failure(let error):","        print(\"Tagging error: \\(error)\")","    }","","} receiveValue: { _ in }","","\/\/ Async\/Await","do {","    try await adService.tagUser(","        request: .init([","            .init(key: \"key\", subkey: \"subkey\", value: \"value\"),","            .init(key: \"key2\", subkey: \"subkey2\")","        ])","    )","","    print(\"Tagging finished\")","","} catch {","    print(\"Tagging error: \\(error)\")","}"],"type":"codeListing"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Conforms to the "},{"type":"codeVoice","code":"Equatable"},{"type":"text","text":" protocol."}]}],"kind":"content"}],"sections":[],"metadata":{"modules":[{"name":"AdSDKCore"}],"roleHeading":"Structure","symbolKind":"struct","role":"symbol","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"TagRequest"}],"navigatorTitle":[{"text":"TagRequest","kind":"identifier"}],"title":"TagRequest","externalID":"s:9AdSDKCore10TagRequestV"},"identifier":{"url":"doc:\/\/com.adsdk.core\/documentation\/AdSDKCore\/TagRequest","interfaceLanguage":"swift"},"topicSections":[{"identifiers":["doc:\/\/com.adsdk.core\/documentation\/AdSDKCore\/TagRequest\/Tag"],"anchor":"Str