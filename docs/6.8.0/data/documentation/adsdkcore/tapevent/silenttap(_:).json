e process redirect and tap as a separate processes.","    await withTaskGroup(","        of: Void.self","    ) { [weak adControllerDelegate] group in","        group.addTask {","            \/\/ Process redirect","            try? await adControllerDelegate?.performTap(","                .silentTap(url)","            )","        }","","        group.addTask {","            \/\/ Process tap","            try? await adControllerDelegate?.performTap(","                .tapURL(.init(url))","            )","        }","","        try await group.waitForAll()","    }","}"],"type":"codeListing"}]}],"sections":[],"references":{"doc://com.adsdk.core/documentation/AdSDKCore":{"kind":"symbol","type":"topic","identifier":"doc:\/\/com.adsdk.core\/documentation\/AdSDKCore","title":"AdSDKCore","role":"collection","abstract":[{"text":"Manage, display, and create your custom advertisements on iOS using cutting-edge technologies such as Structured Concurrency, and SwiftUI.","type":"text"}],"url":"\/documentation\/adsdkcore"},"doc://com.adsdk.core/documentation/AdSDKCore/TapEvent/silentTap(_:)":{"identifier":"doc:\/\/com.adsdk.core\/documentation\/AdSDKCore\/TapEvent\/silentTap(_:)","kind":"symbol","url":"\/documentation\/adsdkcore\/tapevent\/silenttap(_:)","role":"symbol","fragments":[{"text":"case","kind":"keyword"},{"text":" ","kind":"text"},{"text":"silentTap","kind":"identifier"},{"text":"(","kind":"text"},{"text":"URL","preciseIdentifier":"s:10Foundation3URLV","kind":"typeIdentifier"},{"text":")","kind":"text"}],"type":"topic","abstract":[{"text":"Processes the redirect of the passed URL without processing the target URL.","type":"text"}],"title":"TapEvent.silentTap(_:)"},"doc://com.adsdk.core/documentation/AdSDKCore/TapEvent":{"navigatorTitle":[{"text":"TapEvent","kind":"identifier"}],"identifier":"doc:\/\/com.adsdk.core\/documentation\/AdSDKCore\/TapEvent","title":"TapEvent","fragments":[{"text":"enum","kind":"keyword"},{"text":" ","kind":"text"},{"text":"TapEvent","kind":"ide